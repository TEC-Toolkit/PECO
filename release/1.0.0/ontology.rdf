    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/hasFeatureOfInterest -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/hasFeatureOfInterest">
        <rdfs:comment>A relation between an Observation and the entity whose quality was observed, or between an Actuation and the entity whose property was modified, or between an act of Sampling and the entity that was sampled.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label>has feature of interest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/hasResult -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/hasResult">
        <rdfs:comment>Relation linking an Observation and a Sensor or Actuator and a Result, which contains a value representing the value associated with the observed Property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label>has result</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sosa/madeBySensor -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/madeBySensor">
        <rdfs:comment>Relation between an Observation and the Sensor which made the Observations.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label>made by Sensor</rdfs:label>
    </owl:ObjectProperty>
    


