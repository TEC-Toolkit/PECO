        <rdfs:range rdf:resource="https://w3id.org/peco#EmissionScore"/>
        <rdfs:comment>A property linking peco:EmissionGenerationActivity with a quantifiable representation of the amount of emissions prodcued by this activity.</rdfs:comment>
        <rdfs:label>has emission score</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/peco#inEmissionActivityContext -->


    <owl:ObjectProperty rdf:about="https://w3id.org/peco#inEmissionActivityContext">
