        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/sosa/"/>
        <rdfs:label>phenomenon time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/peco#hasEmissionScore -->


    <owl:ObjectProperty rdf:about="https://w3id.org/peco#hasEmissionScore">
